      </div>

      </div>

      <h3> </h3>


      <div class="container3">
        <div class="container2">
          <h4>
            Our Collection
          </h4>
        </div>
        <div class="movie-deck">


          <div class="movie-deck">
            <% 
            // Assuming you want to display a random selection of 8 movies
            // Shuffle the array of movies randomly
            const shuffledMovies = movies.sort(() => Math.random() - 0.5);
        
            // Select the first 6 movies from the shuffled array
            shuffledMovies.slice(0, 6).forEach(movie => { 
            %>
            <div class="movie">
                <a href="/movies/<%= movie._id %>">
                    <img src="<%= movie.image %>" alt="<%= movie.title %>" />
                    <p><%= movie.title %></p>
                </a>
            </div>
            <% }); %>
        </div>









          <div class="container5">
